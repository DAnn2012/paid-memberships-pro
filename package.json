{
  "name": "paid-memberships-pro",
  "version": "2.10.1",
  "description": "WordPress Membership Plugin",
  "main": "webpack.config.js",
  "directories": {
    "test": "tests"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/strangerstudios/paid-memberships-pro.git"
  },
  "scripts": {
    "start": "npm run watch:sass & wp-scripts start",
