    "watch:sass": "sass --watch ./scss/main.scss:css/pmpro-main.css"
  },
  "keywords": [
    "wordpress",
    "membership",
    "pmpro"
  ],
  "author": "Stranger Studios",
  "license": "GPL-2.0",
  "devDependencies": {
    "@wordpress/scripts": "^23.7.2",
    "node-sass": "^7.0.3",
    "sass": "^1.62.1"
  }
}
